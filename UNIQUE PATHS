#include <bits/stdc++.h> 
int uniquePaths(int m, int n) {
    int a=1;
    for(int i=1;i<=n-1;i++)
    {
        a=(a*(m+i-1))/i;
    }
    return a;
}



APPROACVH-2
#include <bits/stdc++.h> 
int count(int n,int m,vector<vector<int>> &dp)
    {
        if(n==1||m==1)
        {
            return 1;
        }
     
        if(n==0||m==0)
        {
            return 0;
        }
     
        if(dp[m][n]!=-1)
        {
            return dp[m][n];
        }
     
         return  dp[m][n]=count(n-1,m,dp)+count(n,m-1,dp);
        
    }

    int uniquePaths(int m, int n) {
        vector<vector<int>> dp(m+1,vector<int>(n+1,-1));
     
        return count(n,m,dp);
        
    }
